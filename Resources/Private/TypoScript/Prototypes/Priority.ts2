prototype(XML:Priority) < prototype(TS:Tag) {
	@context.priority = TS:Value {
		xmlSitemapPriority = ${q(node).property('xmlSitemapPriority')}
		value = ${this.xmlSitemapPriority == '' ? configuration.defaultPriority : this.xmlSitemapPriority}
	}
	tagName = 'priority'
	content = ${priority}
	@if.hasValue = ${priority != false}
}
